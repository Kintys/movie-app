/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": [],
    "paths": {
      "@/*": ["./src/*"],
      "@environment/*": ["./src/environments/*"],
      "@asset/*": ["./src/assets/*"],
      "@pages/*": ["./src/app/pages/*"],
      "@store/*": ["./src/app/store/*"]
      // "@core/*": ["./src/app/core/*"],
      // "@shared/*": ["./src/app/shared/*"]
    }
  },
  "files": ["src/main.ts"],
  "include": ["src/**/*.d.ts"]
}
